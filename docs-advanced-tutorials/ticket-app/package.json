  "main": "frontend/index.js",
  "scripts": {
    "build": "yarn run build:web",
    "build:web": "parcel build frontend/index.html --public-url ./",
    "deploy": "yarn run build:contract && near dev-deploy",
    "start": "cd frontend && parcel index.html --open",
    "dev": "nodemon --watch contract -e ts --exec \"yarn run start\"",
    "test": "yarn run build:contract && yarn run test:unit && yarn run test:integration",
    "test:unit": "cd contract && cargo test",
    "test:integration": "yarn run deploy && cd integration-tests && yarn run test",
    "test:integration:ts": "echo not supported"
  },
  "devDependencies": {
    "@babel/core": "~7.18.2",
    "@babel/preset-env": "~7.18.2",
    "@babel/preset-react": "~7.17.12",
    "ava": "^4.2.0",
    "env-cmd": "~10.1.0",
    "near-cli": "~3.3.0",
    "nodemon": "~2.0.16",
    "parcel": "^2.6.0",
    "process": "^0.11.10",
    "react-test-renderer": "~18.1.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "assert": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "os-browserify": "^0.3.0",
    "stream-browserify": "^3.0.0"
  },
  "dependencies": {
    "keypom-js": "^1.4.1-rc.3",
    "qrcode.react": "^3.1.0",
    "near-api-js": "~0.44.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "~0.13.9",
    "react-router-dom": "^6.8.2",
    "react-zxing": "^1.1.3",
    "web-vitals": "^3.1.1"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
